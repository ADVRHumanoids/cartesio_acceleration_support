<launch>

    <!-- include file="$(find cartesian_interface)/examples/launch/coman.launch"/ -->

    <param name="/cartesian/problem_description"
                   textfile="$(find cartesio_acceleration_support)/examples/config/coman_stack_force.yaml"/>

    
    <arg name="prefix" default=""/>
    <arg name="gui" default="true"/>
    
    <param name="robot_description" 
        textfile="$(find cartesian_interface)/examples/urdf/coman.urdf"/>
    
    <param name="robot_description_semantic"
        textfile="$(find cartesian_interface)/examples/srdf/coman.srdf"/>
    
    
    
    <include file="$(find cartesian_interface)/launch/cartesio.launch">
        <arg name="rate" value="100.0"/>
        <arg name="prefix" value="$(arg prefix)"/>
    </include>

    <node if="$(arg gui)"
            type="rviz"
            name="rviz"
            pkg="rviz"
            args="-d $(find cartesio_acceleration_support)/examples/rviz/coman_force.rviz"/>
            output="screen"/>

</launch>
